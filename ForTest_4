import java.util.Scanner;

public class FotTest_4 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int num = 0;
        do{
            System.out.print("10 이하의 정수를 입력하세요 : ");
            num = sc.nextInt();
        }while(num > 10);

        for(int i = 1; i <= num; i++){
            System.out.println("MSG");
        }
    }
}
