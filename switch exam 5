import java.util.Scanner;

public class SwitchExam5 {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        System.out.print("학급을 입력하세요 : ");
        int classroom = sc.nextInt();
        if(classroom > 6) {
            do {
                System.out.print("학급을 다시 입력하세요 : ");
                classroom = sc.nextInt();
            } while (6 < classroom);
        }
        switch(classroom) {
            case 1, 2, 3, 4 -> System.out.println("뉴미디어소프트웨어과");
            case 5, 6 -> System.out.println("뉴미디어디자인과");
        }
    }
}
